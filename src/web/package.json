{
	"name": "website",
	"version": "1.0.0",
	"author": "0x258",
	"license": "ISC",
	"scripts": {
		"dev": "cross-env NODE_ENV=development vue-cli-service serve --mode default",
		"pro": "cross-env NODE_ENV=production vue-cli-service serve --mode default",
		"build": "cross-env NODE_ENV=production vue-cli-service build --mode default",
		"build:dev": "cross-env NODE_ENV=development vue-cli-service build --mode default",
		"lint": "vue-cli-service lint --ext .js,.vue",
		"fix": "vue-cli-service lint --fix --ext .js,.vue"
	},
	"dependencies": {
		"babel-eslint": "^10.1.0",
		"core-js": "^2.6.5",
		"cross-env": "^7.0.3",
		"vue-i18n": "8.26.7"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.5.0",
		"@vue/cli-plugin-eslint": "~4.5.0",
		"@vue/cli-service": "^3.8.0",
		"autoprefixer": "^9.8.8",
		"babel-plugin-component": "^1.1.1",
		"babel-preset-es2015": "^6.24.1",
		"compression-webpack-plugin": "5.0.1",
		"eslint": "^6.7.2",
		"eslint-plugin-vue": "^6.2.2",
		"postcss": "^7.0.39",
		"tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
		"terser-webpack-plugin": "4.2.3",
		"vue-template-compiler": "^2.6.10"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {
			"vue/comment-directive": "off"
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	]
}
